{
  "env": {
    "browser": true,
    "es6": true
  },
  "globals": {
    "ENV": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "indent": [
        2
    ],
    "linebreak-style": [
        2,
        "unix"
    ],
    "quotes": [
        0,
        "single"
    ],
    "semi": [
        2,
        "always"
    ],
    "no-undef": 0
  }
}